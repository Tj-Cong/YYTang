.head_logo {
  margin-top: 50rpx;
  height: 200rpx;
  margin-top: 0;
}
.head_img {
  width: 700rpx;
  height: 200rpx;
  margin: auto;
}

.search-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 50rpx;
  width: 100%;
}
.row_radio_group {
  display: flex;
  flex-direction: row;
  margin: 40rpx auto;
}

radio {
  letter-spacing: 4pt;
  margin: auto 15px;
}

.t-class-input {
  width: 80%;
}

page .t-class-cancel {
  color: #0052d9;
  font-weight: 400;
}

.demo-block__wrapper {
  margin-top: 32rpx;
  width: 100%;
  display: grid;
  align-items: center;
}
.demo-1 {
  margin: 0 32rpx;
}
.t-class-content {
  margin-right: 20rpx;
  margin-left: 20rpx;
}

.picker1 {
  display: flex;
  flex-direction: row;
  margin: 32rpx;
}

.pannel-label {
  margin-right: 10rpx;
  font-size: 28rpx;
}
.pannel-item {
  display: flex;
  align-items: center;
  height: 96rpx;
  padding: 0 20rpx;
  font-size: 28rpx;
  margin-bottom: 32rpx;
  background-color: #fff;
}
.pannel-text {
  flex: 1;
  width: 110rpx;
  color: #000;
  opacity: 0.9;
}
.pannel-text.empty {
  opacity: 0.26;
}
.pannel-select {
  display: flex;
  flex-direction: row;
  width: 160rpx;
  padding: 5px 10px;
  border-style: solid;
  border-width: 1px;
  border-radius: 5px;
}